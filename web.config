<configuration>
  <location path=".">
    <system.webServer>
      <httpProtocol>
        <customHeaders>
          <add name="X-Content-Type-Options" value="nosniff" />
        </customHeaders>
        </httpProtocol>
        <staticContent>
          <mimeMap fileExtension=".*" mimeType="application/octet-stream" />
          <mimeMap fileExtension="." mimeType="text/plain" />
        </staticContent>
      <directoryBrowse enabled="false" />
    </system.webServer>
  </location>
</configuration>